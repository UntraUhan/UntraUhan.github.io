source "https://rubygems.org"

# Используем версию Jekyll, совместимую с GitHub Pages
gem "github-pages", group: :jekyll_plugins

# 🔹 Дополнительные плагины (можно оставить)
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
end
